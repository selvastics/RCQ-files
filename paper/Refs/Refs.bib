@article{compas_effortful_1997,
series = {The {Plenum} series on stress and coping.},
title = {Effortful and involuntary responses to stress: {Implications} for coping with chronic stress.},
issn = {0-306-45470-X (Hardcover)},
doi = {10.1007/978-1-4757-9862-3_4},
abstract = {Discusses effortful and involuntary reactions to stress, focusing on the implications for understanding adjustment to chronic stress. The authors argue that while the concept of coping is limited to effortful responses to stress, we also must examine the complex relationships between coping and involuntary or overlearned patterns of responding to stress. Involuntary processes frequently constrain the types of coping responses that can be generated and, in turn, effortful coping responses may unintentionally initiate involuntary processes that interfere with effective coping.  The authors also distinguish between engagement (involvement with the stressor or one's mood) and disengagement (orienting away from the stressor or associated mood) responses that are both effortful and involuntary as a further step in delineating patterns of coping with chronic stress. These responses differ substantially in their efficacy in managing chronic stress. A critical distinction is also made between 2 types of effortful disengagement coping—avoidance and distraction. The former is typically associated with poor adjustment, whereas the latter most often has beneficial effects. Examples from research on psychological and physical health outcomes are discussed, with a specific focus on the association of effortful and involuntary stress responses in research on depression and cancer. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
journal = {Coping with chronic stress.},
author = {Compas, Bruce E. and Connor, Jennifer and Osowiecki, Dana and Welch, Amy},
year = {1997},
note = {Place: New York,  NY,  US
Publisher: Plenum Press},
keywords = {*Chronic Stress, *Coping Behavior, *Stress, Stress Reactions},
pages = {105-130},
}

@article{kato2015,
author = {Kato, Tsukasa},
title = {Frequently Used Coping Scales: A Meta-Analysis},
journal = {Stress and Health},
volume = {31},
number = {4},
pages = {315-323},
keywords = {COPE, coping behaviour, questionnaire, meta-analysis, ways of coping questionnaire},
doi = {10.1002/smi.2557},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/smi.2557},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/smi.2557},
abstract = {Abstract This article reports the frequency of the use of coping scales in academic journals published from 1998 to 2010. Two thousand empirical journal articles were selected from the EBSCO database. The COPE, Ways of Coping Questionnaire, Coping Strategies Questionnaire, Coping Inventory for Stressful Situations, Religious-COPE and Coping Response Inventory were frequently mentioned. In particular, the COPE (20.2\%) and Ways of Coping Questionnaire (13.6\%) were used the most frequently. In this literature reviewed, coping scales were most often used to assess coping with health issues (e.g. illness, pain and medical diagnoses) over other types of stressors, and patients were the most frequent participants. Further, alpha coefficients were estimated for the COPE subscales, and correlations between the COPE subscales and coping outcomes were calculated, including depressive symptoms, anxiety, negative affect, psychological distress, physical symptoms and well-being. Copyright © 2013 John Wiley \& Sons, Ltd.},
year = {2015}
}


@article{luthar2000,
author = {Luthar, Suniya S. and Cicchetti, Dante and Becker, Bronwyn},
title = {The Construct of Resilience: A Critical Evaluation and Guidelines for Future Work},
journal = {Child Development},
volume = {71},
number = {3},
pages = {543-562},
doi = {10.1111/1467-8624.00164},
url = {https://srcd.onlinelibrary.wiley.com/doi/abs/10.1111/1467-8624.00164},
eprint = {https://srcd.onlinelibrary.wiley.com/doi/pdf/10.1111/1467-8624.00164},
abstract = {This paper presents a critical appraisal of resilience, a construct connoting the maintenance of positive adaptation by individuals despite experiences of significant adversity. As empirical research on resilience has burgeoned in recent years, criticisms have been levied at work in this area. These critiques have generally focused on ambiguities in definitions and central terminology; heterogeneity in risks experienced and competence achieved by individuals viewed as resilient; instability of the phenomenon of resilience; and concerns regarding the usefulness of resilience as a theoretical construct. We address each identified criticism in turn, proposing solutions for those we view as legitimate and clarifying misunderstandings surrounding those we believe to be less valid. We conclude that work on resilience possesses substantial potential for augmenting the understanding of processes affecting at-risk individuals. Realization of the potential embodied by this construct, however, will remain constrained without continued scientific attention to some of the serious conceptual and methodological pitfalls that have been noted by skeptics and proponents alike.},
year = {2000}
}
@article{hjemdal_mestring_2001,
title = {Mestring og psykologisk motstandsdyktighet hos voksne: Utvikling og foreløpig validering av et nytt instrument. [Preliminary results from the development and validation of a Norwegian scale for measuring adult resilience.]},
volume = {38},
issn = {0332-6470(Print)},
abstract = {Reports on the development of a Norwegian instrument measuring adult resilience that may circumvent shortcomings in previous instruments developed in the US. 29 articles were reviewed to identify characteristics, abilities and coping experiences typical of resilient individuals. From this review, 280 statements were generated. Following a linguistic revision, 85 statements were excluded. The remaining 195 statements were given to 139 students. A principal component analysis further reduced the instrument to 45 items, covering five dimensions: personal competence, social competence, social support, family coherence, and personal structure. The internal consistencies in terms of Cronbach's alphas were high for all dimensions, ranging from .92–.74, and for all the items combined (total Cronbach's alpha = .93). Further studies are under way to examine the construct and predictive validities of this instrument. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
