@article{rice_personal_2016,
title = {Personal resilience and coping with implications for work. {Part} {I}: {A} review},
volume = {54},
issn = {1875-9270},
doi = {10.3233/WOR-162300},
abstract = {BACKGROUND: Interest in resilience has increased in recent years. The U.S. military focus is on personal health and adaptation following exposure to battle, while the civilian interest centers on adjustments subsequent to disastrous events. Coping skills are also relevant, yet the relationships between coping and resilience are unclear. OBJECTIVE: This brief review examines personal resilience and individual coping strategies, exploring definitions of each, along with their potential relationships to one another. Their potential contributions within a work setting are described. METHODS: A literature review was conducted using search terms of resilience, resiliency, personal resilience, coping and resilient coping. RESULTS: Coping refers to one’s using purposeful actions to handle life situations. Coping techniques can be functional or dysfunctional and the situations one copes with may be acute or long term, severe or minor. Resilience refers to positive and functional handling of oneself and ones’ life, referring to the ability to recover, recuperate, and regenerate following tragic events. CONCLUSIONS: While coping and resilience are related to one another, they are distinct concepts. Positive coping techniques may contribute to resilience. However, which coping techniques improve resilience, and in what circumstances, are questions for future research.},
number = {2},
journal = {Work},
author = {Rice, Valerie and Liu, Baoxia},
year = {2016},
note = {Publisher: IOS Press},
keywords = {active duty, training, Trauma, veteran},
pages = {325-333},
}




@article{schwarzer_positive_2003,
	title = {Positive coping: {Mastering} demands and searching for meaning.},
	issn = {1-55798-988-5 (Hardcover)},
	doi = {10.1037/10612-025},
	abstract = {This chapter is organized into two parts, using the terms mastery and meaning as proxies for the two broad processes of coping. Mastery pertains to problem-focused or assimilative coping with demands, whereas meaning refers to accommodative coping. These coping processes need not be applied exclusively, as they may occur more or less simultaneously. The focus of this chapter is on an innovative theoretical perspective that emphasizes positive coping and expands on previous approaches. The first section distinguishes between four kinds of adaptation: reactive coping, anticipatory coping, proactive coping, and preventive coping. The next section deals with situational and global approaches to the role of meaning in the coping process. Assessment approaches that try to tap innovative aspects of positive coping are (a) the mastery of future threats or challenges, as reflected by preventive or proactive coping, and (b) the search for meaning. Coping measurement is complicated by a number of conceptual issues, some of which pertain to the stability, generality, and dimensionality of coping. The Proactive Coping Scale is appended. (PsycInfo Database Record (c) 2023 APA, all rights reserved)},
	journal = {Positive psychological assessment:  A handbook of models and measures.},
	author = {Schwarzer, Ralf and Knoll, Nina},
